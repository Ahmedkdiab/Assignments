#include <stdio.h>
#include <string.h>
#define SIZE 10
void separat(char *str1,char *str2,int size);


int main(void){

	char s1[SIZE]="Ahmed";
	char s2[SIZE];
	separat(s1, s2, SIZE);



	return 0;
}

void separat(char *str1,char *str2){
	int i;
	char *ptr1=str1;
	char *ptr2=str2;

	while(*ptr1!='\0'){
		*(ptr1+i)=ptr2[0];
		*(ptr1+i)=ptr2[1];


	}

	printf("%s\t",str2[i]);

}

