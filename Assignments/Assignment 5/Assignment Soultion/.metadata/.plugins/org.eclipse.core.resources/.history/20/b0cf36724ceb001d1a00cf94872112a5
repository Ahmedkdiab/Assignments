#include <stdio.h>

struct student{
	char *first_name;
	char *second_name;
	int mark;

}s1,*s_ptr;


int main(void){
	s_ptr=&s1;

	s_ptr->first_name="Ahmed";
	s_ptr->second_name="Khaled";
	s_ptr->mark=90;

	printf("Student 1 Information:\n");
	printf("Name: %s %s\nMaraks: %d\n",s_ptr->first_name,s_ptr->second_name,s_ptr->mark);


	return 0;
}

